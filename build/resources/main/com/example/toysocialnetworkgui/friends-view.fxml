<AnchorPane prefHeight="644.0" prefWidth="928.0" style="-fx-background-color: #F0E68C;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.toysocialnetworkgui.FriendsListController">
   <children>
      <ListView fx:id="friendsList" layoutX="424.0" layoutY="180.0" prefHeight="353.0" prefWidth="300.0" />
      <AnchorPane prefHeight="118.0" prefWidth="936.0" style="-fx-background-color: #191970;">
         <children>
            <Label fx:id="MicroLabel" layoutX="29.0" layoutY="14.0" prefHeight="91.0" prefWidth="159.0" text="Micro" textFill="#f8f4f4">
               <font>
                  <Font size="55.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="757.0" layoutY="23.0" mnemonicParsing="false" onAction="#backButtonClick" prefHeight="72.0" prefWidth="142.0" text="BACK">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button fx:id="deleteFriendButton" onAction="#deleteButtonClick" layoutX="497.0" layoutY="556.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="142.0" text="Delete" />
      <Label layoutX="60.0" layoutY="153.0" prefHeight="55.0" prefWidth="228.0" text="List Of Friends">
         <font>
            <Font size="24.0" />
